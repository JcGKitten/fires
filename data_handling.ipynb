    "# normalize mnist data\n",
    "import pandas as pd\n",
    "df = pd.read_csv('datasets/Multiclass/mnist_train.csv')\n",
    "\n",
    "df_class = df[\"0\"]\n",
    "df /= 255\n",
    "df['0'] = df_class\n",
    "\n",
    "df.to_csv('datasets/Multiclass/mnist_train.normalized.csv', index=False)"
